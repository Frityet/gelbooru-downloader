{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb-dap",
            "request": "launch",
            "name": "Launch",
            "program": "${workspaceRoot}/build/linux/x86_64/debug/gelbooru-downloader",
            "args": [
                "-o", "./",
                "-t", "highres"
            ],
            "cwd": "${workspaceRoot}",
            "disableASLR": false,
            "initCommands": [
                "settings set target.language objc",
                "settings set target.disable-aslr false"
            ],
        }
    ],
    "compounds": []
}